{
    "name": "faustbrian/byte-buffer",
    "description": "A fast and simple ByteBuffer implementation using SplFixedArray.",
    "keywords": ["byte", "buffer", "pack", "unpack", "binary", "hex"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://github.com/faustbrian"
    }],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "illuminate/support": "^7.0.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0.0",
        "spatie/phpunit-watcher": "^1.5",
        "symfony/var-dumper": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\ByteBuffer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\ByteBuffer\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test-watch": [
            "vendor/bin/phpunit-watcher watch"
        ]
    }
}
